{
  "name": "gsti-empleado",
  "version": "0.0.3",
  "main": "index.ts",
  "type": "module",
  "engines": {
    "node": ">=20.13.1",
    "npm": ">=10.5.2"
  },
  "scripts": {
    "start": "expo start --clear",
    "web": "expo start --web",
    "web:export": "expo export --platform web",
    "version:sync": "node scripts/sync-version.js",
    "pwa:copy": "cp public/manifest.json dist/ && cp public/service-worker.js dist/ && cp -r public/assets/* dist/assets/ 2>/dev/null || true && node scripts/inject-pwa-meta.js",
    "start:sae:production": "ENVFILE=config/sae-production/.env expo start --clear",
    "web:sae:production": "ENVFILE=config/sae-production/.env expo start --web",
    "build:sae:preview": "npm run version:sync && ENVFILE=config/sae-production/.env eas build --profile preview",
    "build:sae:preview:local": "npm run version:sync && ENVFILE=config/sae-production/.env eas build --profile preview --local",
    "build:sae:production": "npm run version:sync && ENVFILE=config/sae-production/.env eas build --profile production",
    "build:web": "npm run version:sync && ENVFILE=config/sae-production/.env expo export --platform web && npm run pwa:copy",
    "update:sae:preview": "ENVFILE=config/sae-production/.env eas update --branch preview",
    "update:sae:production": "ENVFILE=config/sae-production/.env eas update --branch production",
    "start:gsti-demo:production": "ENVFILE=config/gsti-demo-production/.env expo start --clear",
    "web:gsti-demo:production": "ENVFILE=config/gsti-demo-production/.env expo start --web",
    "build:gsti-demo:preview": "npm run version:sync && ENVFILE=config/gsti-demo-production/.env eas build --profile preview",
    "build:gsti-demo:preview:local": "npm run version:sync && ENVFILE=config/gsti-demo-production/.env eas build --profile preview --local",
    "build:gsti-demo:production": "npm run version:sync && ENVFILE=config/gsti-demo-production/.env eas build --profile production",
    "build:gsti-demo:web": "npm run version:sync && ENVFILE=config/gsti-demo-production/.env expo export --platform web && npm run pwa:copy",
    "update:gsti-demo:preview": "ENVFILE=config/gsti-demo-production/.env eas update --branch preview",
    "update:gsti-demo:production": "ENVFILE=config/gsti-demo-production/.env eas update --branch production",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "lint": "tsc --noEmit && eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "lint:watch": "nodemon --config nodemon.json",
    "format": "prettier --write \"**/*.{ts,tsx,json,md}\"",
    "prepare": "husky install",
    "docs": "typedoc --options typedoc.json"
  },
  "dependencies": {
    "@expo/metro-runtime": "~5.0.4",
    "@gorhom/bottom-sheet": "5.1.6",
    "@react-native-async-storage/async-storage": "2.2.0",
    "@react-native-community/datetimepicker": "8.4.4",
    "@react-navigation/native": "^7.1.9",
    "@react-navigation/native-stack": "^7.3.13",
    "@turf/turf": "^7.3.1",
    "axios": "^1.9.0",
    "dotenv": "^17.2.1",
    "expo": "^54.0.0",
    "expo-camera": "~17.0.9",
    "expo-constants": "~18.0.10",
    "expo-device": "~8.0.9",
    "expo-file-system": "~19.0.19",
    "expo-local-authentication": "~17.0.7",
    "expo-localization": "~17.0.7",
    "expo-location": "~19.0.7",
    "expo-random": "^14.0.1",
    "expo-secure-store": "~15.0.7",
    "expo-status-bar": "~3.0.8",
    "expo-updates": "~29.0.12",
    "face-api.js": "^0.22.2",
    "luxon": "^3.6.1",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-i18next": "^15.5.1",
    "react-native": "0.81.5",
    "react-native-dotenv": "^3.4.11",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-paper": "^5.14.5",
    "react-native-reanimated": "4.1.5",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-svg": "15.12.1",
    "react-native-web": "^0.21.2",
    "react-native-worklets": "0.5.1"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@eslint/js": "^9.27.0",
    "@types/i18next": "^13.0.0",
    "@types/luxon": "^3.6.2",
    "@types/react": "~19.1.10",
    "@types/react-native-vector-icons": "^6.4.18",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.32.1",
    "@typescript-eslint/parser": "^8.32.1",
    "braintree-jsdoc-template": "^3.3.0",
    "eslint": "^9.27.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.4.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-unicorn": "^59.0.1",
    "husky": "^9.1.7",
    "jsdoc": "^4.0.4",
    "jsdoc-plugin-typescript": "^3.2.0",
    "nodemon": "^3.1.10",
    "prettier": "^3.5.3",
    "taffydb": "^2.7.3",
    "typedoc": "^0.28.4",
    "typedoc-plugin-markdown": "^4.6.3",
    "typescript": "~5.9.2"
  },
  "private": true
}
